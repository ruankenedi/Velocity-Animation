{
    "extends": "airbnb-base",
    "rules": {
        "comma-dangle": ["error", {
            "arrays": "never",
            "objects": "never",
            "imports": "never",
            "exports": "never",
            "functions": "ignore"
        }],
        "linebreak-style": 0,
        "functionPrototypeMethods": true
    }
}